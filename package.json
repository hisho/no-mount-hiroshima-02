{
  "name": "tailwind",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev -p 3000",
    "build": "run-s build:next",
    "build:next": "next build",
    "start": "next start",
    "lint": "run-p lint:*",
    "lint:next": "next lint --dir src --ignore-path .prettierignore --max-warnings 0",
    "lint:typecheck": "tsc --pretty --noEmit",
    "lint:prettier": "prettier --check .",
    "format": "run-s format:*",
    "format:eslint": "pnpm lint:next --fix",
    "format:prettier": "pnpm lint:prettier --write"
  },
  "dependencies": {
    "@next/font": "13.4.19",
    "@t3-oss/env-nextjs": "^0.6.1",
    "clsx": "^2.0.0",
    "next": "13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-use": "^17.4.0",
    "tailwind-merge": "^1.14.0",
    "ts-pattern": "^5.0.5",
    "typesafe-utils": "^1.16.2",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.1",
    "@types/lodash": "^4.14.197",
    "@types/node": "20.5.4",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/react-syntax-highlighter": "^15.5.7",
    "autoprefixer": "^10.4.15",
    "dotenv": "^16.3.1",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.19",
    "eslint-plugin-perfectionist": "^1.5.1",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.28",
    "prettier": "^3.0.2",
    "prettier-plugin-tailwindcss": "0.4.0",
    "tailwindcss": "^3.3.3",
    "typescript": "5.1.6"
  },
  "volta": {
    "node": "18.16.1"
  }
}
